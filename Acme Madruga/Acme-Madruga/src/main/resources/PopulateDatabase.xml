<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	">
	
	<!-- Other domain beans come here -->	
	
	
	<bean id="userAccount1" class="security.UserAccount">
		<property name="username" value="admin1" />
		<property name="password" value="21232f297a57a5a743894a0e4a801fc3" />
		<property name="authorities">
			<list>
				<bean class="security.Authority">
					<property name="authority" value="ADMIN" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="actor1" class="domain.Admin">
		<property name="name" value="Admin" />
		<property name="surname" value="Number One" />
		<property name="photo" value="https://pngimage.net/wp-content/uploads/2018/06/logo-admin-png-4.png" />
		<property name="email" value="admin1@" />
		<property name="phoneNumber" value="698765432" />
		<property name="userAccount" ref="userAccount1" />
	</bean>

	<bean id="float1" class="domain.Float">
		<property name="title" value="Gran Poder" />
		<property name="description" value="Gran Poder" />
		<property name="photos">
			<list>
			<value>https://sevilla.abc.es/pasionensevilla//wp-content/uploads/2016/11/gran-poder.jpg</value>
			</list>
		</property>
	</bean>
	
</beans>